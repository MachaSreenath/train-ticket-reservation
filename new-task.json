{
  "family": "train-ticket-family",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::474668386320:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::474668386320:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "train-ticket-repo",
      "image": "474668386320.dkr.ecr.us-east-1.amazonaws.com/train-ticket-repo",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/trainbook",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
