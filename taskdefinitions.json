{
  "family": "trainbook-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::369759350131:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::369759350131:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "trainbook-container",
      "image": "369759350131.dkr.ecr.us-east-1.amazonaws.com/train-ticket-reservation:1763379368",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/trainbook",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
